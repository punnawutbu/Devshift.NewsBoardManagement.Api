{
  "AllowedHosts": "*",
  "Consul": {
    "Port": 8502,
    "Discovery": {
      "ServiceName": "Devshift.NewsBoardManagement.Api",
      "Tags": [
        "dotnet",
        "microservice"
      ],
      "Heartbeat": {
        "Enabled": true
      },
      "HealthCheckPath": "/health",
      "HealthCheckInterval": "30s",
      "HealthCheckCriticalTimeout": "1m",
      "HealthCheckTlsSkipVerify": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Swagger": {
    "Title": "Devshift.NewsBoardManagement.Api",
    "Version": "v1",
    "Name": "PUNN",
    "Email": "support@nio.com",
    "Endpoint": "/swagger/v1/swagger.json"
  },
  "Urls": "http://0.0.0.0:32002"
}